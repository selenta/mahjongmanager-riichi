                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/redDoraCount">

                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/redDora0"
                        android:checked="false" />

                    <RadioButton
